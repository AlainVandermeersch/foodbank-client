    "angular-oauth2-oidc": "^10.0.3",
    "body-parser": "^1.18.2",
    "chart.js": "^3.9.1",
    "core-js": "^3.25.5",
    "express": "^4.16.2",
    "file-saver": "^2.0.5",
    "json-server": "^0.16.3",
    "moment": "^2.29.1",
    "ngx-print": "^1.3.1",
    "primeflex": "^2.0.0",
    "primeicons": "^6.0.1",
