{
  "name": "food-it",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve  --proxy-config ./proxy.json",
    "server": "ts-node -P ./server/server.tsconfig.json ./server/server.ts",
    "build": "ng build",
    "build-nl": "ng build --configuration=nl",
    "build-fr": "ng build --configuration=fr",
    "build-ovh-dev": "ng build --configuration=ovh-dev",
    "build-prod": "ng build --configuration=production",
    "build-docker-local": "ng build --configuration=production,docker-local",
    "build-docker-local-nl": "ng build --configuration=production,docker-local-nl",
    "build-docker-local-fr": "ng build --configuration=production,docker-local-fr",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "api": "json-server src/server/api/dbAlain.json --routes src/server/api/routes.json --middlewares src/server/api/singular.js --no-cors=false --port 8081"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "^15.2.6",
    "@angular/animations": "^15.2.8",
