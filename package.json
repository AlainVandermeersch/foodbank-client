    "@ngrx/store-devtools": "^10.1.2",
    "angular-oauth2-oidc": "^10.0.3",
    "body-parser": "^1.18.2",
    "chart.js": "^2.9.4",
    "core-js": "^2.6.12",
    "express": "^4.16.2",
    "file-saver": "^2.0.5",
    "json-server": "^0.16.3",
    "moment": "^2.29.1",
