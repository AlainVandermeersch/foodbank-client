{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "foodbank-it-client": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "i18n": {
        "locales": {
          "fr-FR": {
            "translation": "src/locale/messages.fr.xlf"
          },
          "nl-NL": {
            "translation": "src/locale/messages.nl.xlf"
          }
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "localize": true,
            "aot": true,
            "i18nMissingTranslation": "error",
            "baseHref": "/",
            "outputPath": "dist/foodbank-it-client/",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ],
            "styles": [
              "src/styles.scss",
              "node_modules/quill/dist/quill.core.css",
              "node_modules/quill/dist/quill.snow.css"
            ],
            "scripts": [
              "node_modules/quill/dist/quill.min.js",
              "node_modules/chart.js/dist/chart.min.js"],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "development": {
            },
            "ovh-dev": {
              "baseHref": "/",
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.dev.ts"
                }
              ]
            },
            "production": {
              "baseHref": "/",
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "fr": {
              "baseHref": "/",
              "outputPath": "dist/foodbank-it-client-fr/",
              "localize": ["fr"],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.fr.ts"
                }
              ]
            },
            "nl": {
              "baseHref": "/",
              "outputPath": "dist/foodbank-it-client-nl/",
              "localize": ["nl"],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.nl.ts"
                }
              ]
            },
            "docker-local": {
              "baseHref": "/",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.docker-local.ts"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "foodbank-it-client:build"
          },
          "configurations": {
            "development": {
              "browserTarget": "foodbank-it-client:build:development"
            },
            "production": {
              "browserTarget": "foodbank-it-client:build:production"
            },
            "fr": {
              "browserTarget": "foodbank-it-client:build:fr"
            },
            "nl": {
              "browserTarget": "foodbank-it-client:build:nl"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "foodbank-it-client:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "src/styles.scss"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ]
          }
        }
      }
    },
    "foodbank-it-client-e2e": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "foodbank-it-client:serve"
          }
        }
      }
    }
  },
  "defaultProject": "foodbank-it-client",
  "schematics": {
    "@ngrx/schematics:component": {
      "prefix": "",
      "styleext": "scss"
    },
    "@ngrx/schematics:directive": {
      "prefix": ""
    }
  },
  "cli": {
    "defaultCollection": "@ngrx/schematics",
    "analytics": "f55f4a8f-5057-4bd1-bc62-645d74960959"
  }
}