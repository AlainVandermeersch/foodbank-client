{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "foodbank-it-client": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "i18n": {
        "sourceLocale": "en-US",
        "locales": {
          "fr": "src/locale/messages.fr.xlf",
          "nl": "src/locale/messages.nl.xlf"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "baseHref": "/en/",
            "aot": true,
            "outputPath": "dist/foodbank-it-client-en/",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ],
            "styles": [
              "src/styles.scss",
              "node_modules/quill/dist/quill.core.css",
              "node_modules/quill/dist/quill.snow.css"
            ],
            "scripts": ["node_modules/quill/dist/quill.min.js"]
          },
          "configurations": {
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            },
            "production": {
              "baseHref": "/en/",
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "prod-fr": {
              "baseHref": "/fr/",
              "aot": true,
              "outputPath": "dist/foodbank-it-client-fr/",
              "i18nFile": "src/locale/messages.fr.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "fr-FR",
              "i18nMissingTranslation": "error",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod-fr.ts"
                }
              ]
            },
            "prod-nl": {
              "baseHref": "/nl/",
              "aot": true,
              "outputPath": "dist/foodbank-it-client-nl/",
              "i18nFile": "src/locale/messages.nl.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "nl-NL",
              "i18nMissingTranslation": "error",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod-nl.ts"
                }
              ]
            },
            "fr": {
              "baseHref": "/fr/",
              "outputPath": "dist/foodbank-it-client-fr/",
              "i18nFile": "src/locale/messages.fr.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "fr-FR",
              "i18nMissingTranslation": "error",
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.fr.ts"
                }
              ]
            },
            "nl": {
              "baseHref": "/nl/",
              "outputPath": "dist/foodbank-it-client-nl/",
              "i18nFile": "src/locale/messages.nl.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "nl-NL",
              "i18nMissingTranslation": "error",
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.nl.ts"
                }
              ]
            },
            "docker-local": {
              "baseHref": "/en/",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.docker-local.ts"
                }
              ]
            },
            "docker-local-fr": {
              "baseHref": "/fr/",
              "outputPath": "dist/foodbank-it-client-fr/",
              "i18nFile": "src/locale/messages.fr.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "fr-FR",
              "i18nMissingTranslation": "error",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.docker-local-fr.ts"
                }
              ]
            },
            "docker-local-nl": {
              "baseHref": "/nl/",
              "outputPath": "dist/foodbank-it-client-nl/",
              "i18nFile": "src/locale/messages.nl.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "nl-NL",
              "i18nMissingTranslation": "error",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.docker-local-nl.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "foodbank-it-client:build"
          },
          "configurations": {
            "development": {
              "browserTarget": "foodbank-it-client:build:development"
            },
            "production": {
              "browserTarget": "foodbank-it-client:build:production"
            },
            "fr": {
              "browserTarget": "foodbank-it-client:build:fr"
            },
            "nl": {
              "browserTarget": "foodbank-it-client:build:nl"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "foodbank-it-client:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "src/styles.scss"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "foodbank-it-client-e2e": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "foodbank-it-client:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "foodbank-it-client",
  "schematics": {
    "@ngrx/schematics:component": {
      "prefix": "",
      "styleext": "scss"
    },
    "@ngrx/schematics:directive": {
      "prefix": ""
    }
  },
  "cli": {
    "defaultCollection": "@ngrx/schematics",
    "analytics": "f55f4a8f-5057-4bd1-bc62-645d74960959"
  }
}